# =============================================================================
# ezGO POC - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# =============================================================================
# Database
# =============================================================================
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ezgo-poc
POSTGRES_DB=ezgo-poc
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# =============================================================================
# Mapbox (Required for routing)
# =============================================================================
# Get your token at: https://account.mapbox.com/access-tokens/
MAPBOX_ACCESS_TOKEN=
NEXT_PUBLIC_MAPBOX_TOKEN=

# =============================================================================
# Backend
# =============================================================================
SECRET_KEY=your-secret-key-change-in-production
API_V1_STR=/api/v1

# =============================================================================
# Frontend
# =============================================================================
NEXT_PUBLIC_API_URL=http://localhost:8000
